            <a href="{{ nationswell_twitter }}" target="_blank" data-track='{"label": "twitter"}' class="icon icon_twitter"></a> -->
    


<script>
    window.addEventListener('scroll', function () {
      document.body.classList[
        window.scrollY > 100 ? 'add': 'remove'
      ]('scrolled');
    });
</script>

{% endblock %}




